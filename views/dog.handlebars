<div class="panel panel-success" style="width: 100%;">
    <div class="panel-heading" style="background-color: lightgray;">
        <h3 class="panel-title" style="color: green;">
            {{name}}'s Profile!
        </h3>
    </div>
    <div class="panel-body">
        {{#each images}}
            <div class="col-md-4 text-center" style="padding-bottom: .5em;"><a href="/images/{{uniqueId}}"><img src="/public/upload/{{filename}}" style="width: 300px; height: 225px; margin: 1px 1px 1px 1px;" class="img-thumbnail"></a></div>
        {{/each}}
    </div>
</div>