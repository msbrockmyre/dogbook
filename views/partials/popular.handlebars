<div class="panel panel-default" length: 100%;">
    <div class="panel-heading" style="background-color: green;">
        <h3 class="panel-title" style="color: white;">
            Highest Rated Dogs
        </h3>
    </div>
    <div class="panel-body">
        {{#each sidebar.popular}}
            <div class="col-md-4 text-center" style="padding-bottom: .5em;"><a href="/images/{{uniqueId}}"><img src="/public/upload/{{filename}}" style="width: 75px; height: 75px; border-width: 2px; margin-width: 2px 2px 2px 2px;" class="img-thumbnail"></a></div>
        {{/each}}
    </div>
</div>
